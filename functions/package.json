{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "firebase -P dev serve --only functions",
    "shell": "firebase -P dev functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "standalone": "firebase -P dev emulators:exec --only database 'node standalone.js'",
    "updateprice": "firebase -P dev emulators:exec 'node updateprice.js'"
  },
  "engines": {
    "node": "14"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "eslint": "^8.2.0",
    "eslint-plugin-promise": "^5.1.1",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.0",
    "firebase-functions": "^3.16.0",
    "http-cookie-agent": "^1.0.3",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.3.3",
    "firebase-tools": "^9.22.0",
    "lambda-local": "^2.0.0"
  },
  "resolutions": {
    "tar": "^6.1.2"
  },
  "private": true
}
